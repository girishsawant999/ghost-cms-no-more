{{!< default}}

{{> "header"}}
{{!-- nf content section --}}
<section class="nf-page-content-section pb-5">
 {{#post}}
{{#if feature_image}}
<div class="container pt-4">
      <div class="row">
        <div class="col-md-12 feature-image">
          <div class="image-frame image-hover-style-1">
            <a href="{{url}}">
                <img class="lazyload w-100" srcset="{{img_url feature_image size=" s"}} 300w, {{img_url feature_image
                    size="m" }} 600w, {{img_url feature_image size="l" }} 1000w, {{img_url feature_image size="xl" }}
                    2000w" sizes="(max-width: 1000px) 400px, 800px" src="{{img_url feature_image size=" m"}}"
                    alt="{{title}}" loading="lazy" />
            </a>
          </div>
           
          </div>
      </div>
      </div>
      {{/if}}
      {{/post}}

    {{> "nf-page-title"}}

    <div class="container pt-4">
      <div class="row">
        <div class="col-md-12">
          {{#post}}
            <div class="entry-content entry-fullwidth my-1">
                {{content}}
            </div>
            <div class="row">
            <div class="col-md-12">
              {{> "comments"}}
            </div>
           </div>
          {{/post}}
        </div>
      </div>
    </div>
</section>

{{> "cta-section"}}
{{> "footer"}}